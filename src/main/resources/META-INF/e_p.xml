<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                            http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="employee"  class="my.sxl.e_p.Employee" init-method="init" destroy-method="destroy"
          p:id="0011" p:name="sun" p:tel="666666" p:job-ref="job"></bean>
    <bean id="job" class="my.sxl.e_p.Job" init-method="init" destroy-method="destroy"
          p:id="1" p:jobTitle="扫描员"></bean>
</beans>
